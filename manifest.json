{
    "manifest_version": 2,
    "name": "undistract",
    "version": "1.0",

    "description": "Set a time to block distracting sites while you need to focus.",

    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage"
    ],

    "background": {
    	"scripts": [
    		"background.js"
    	]
    },

    "browser_action": {
    	"default_title": "undistract",
    	"default_popup": "popup.html"
    }
}